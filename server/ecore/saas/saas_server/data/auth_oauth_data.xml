<?xml version="1.0"?>
<ecore>
    <data noupdate="1">
        <record id="saas_oauth_provider" model="auth.oauth.provider">
            <field name="name">SaaS</field>
            <field name="auth_endpoint">http://ecore.local/oauth2/auth</field>
            <field name="scope">userinfo</field>
            <field name="validation_endpoint">http://ecore.local/oauth2/tokeninfo</field>
            <field name="data_endpoint"></field>
            <field name="css_class">zocial ecore</field>
            <field name="body">Log in via SaaS Portal</field>
            <field name="enabled" eval="True"/>
        </record>
    </data>
    <data>
        <delete id="auth_oauth.provider_ecore" model="auth.oauth.provider"/>
    </data>
</ecore>
